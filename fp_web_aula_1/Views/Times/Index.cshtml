@model IEnumerable<fp_web_aula_1.Models.Time>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
                <th>
                    @Html.DisplayNameFor(model => model.Nome)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.EnderecoDeEmail)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Bandeira)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Publicado)
                </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Nome)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EnderecoDeEmail)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Bandeira)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Publicado)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
</body>
</html>
